{
    "name": "misantron/dbunit",
    "description": "DbUnit fork for PHPUnit 8",
    "type": "library",
    "keywords": [
        "database",
        "testing",
        "dbunit"
    ],
    "homepage": "https://github.com/misantron/dbunit/",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksandr Ivanov",
            "email": "misantron@gmail.com",
            "role": "developer"
        }
    ],
    "support": {
        "issues": "https://github.com/misantron/dbunit/issues"
    },
    "require": {
        "php": "^7.2",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "phpunit/phpunit": "^8.2",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
